{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#e0e1dd",
          "background": "#1b263b",
          "properties": {
            "style": "folder",
            "mapped_locations": {
              "..": "üìÇ"
            },
            "prefix": "C:/üìÇ/",
            "max_depth": 1
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#e0e1dd",
          "background": "#415a77",
          "properties": {
            "branch_icon": "Ôêò ",
            "display_status": true,
            "fetch_status": true,
            "branch_template": "{{ .HEAD }}"
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#0d1b2a",
